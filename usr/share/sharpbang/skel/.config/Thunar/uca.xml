<?xml version="1.0" encoding="UTF-8"?>
<actions>
  <action>
    <icon>utilities-terminal</icon>
    <name>Open terminal here</name>
    <command>sh -c &apos;cd %f;x-terminal-emulator&apos;</command>
    <description />
    <patterns>*</patterns>
    <directories />
  </action>
  <action>
    <icon>applications-system</icon>
    <name>Check sha512sum</name>
    <command>yad --image=dialog-info --button=OK --title=&quot;Check sha512sum for %n&quot; --text=&quot;$(sha512sum %f)&quot; --borders=20 --center</command>
    <description />
    <patterns>*.iso;*.img;*.sub</patterns>
    <other-files />
  </action>
</actions>
